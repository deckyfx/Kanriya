{
  "name": "gqlserver",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "npm run build:ts && npm run build:webpack && npm run build:css",
    "build:ts": "tsc",
    "build:webpack": "webpack --mode production",
    "build:css": "tailwindcss -i ./wwwroot/css/app.css -o ./wwwroot/css/app.min.css --minify",
    "dev": "concurrently \"npm run watch:ts\" \"npm run watch:webpack\" \"npm run watch:css\"",
    "watch:ts": "tsc --watch",
    "watch:webpack": "webpack --mode development --watch",
    "watch:css": "tailwindcss -i ./wwwroot/css/app.css -o ./wwwroot/css/app.min.css --watch"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.20",
    "@types/node": "^20.0.0",
    "@types/underscore": "^1.13.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "css-loader": "^6.8.1",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.3.3",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.3.5",
    "ts-loader": "^9.5.0",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.4.2",
    "alpinejs": "^3.13.2",
    "axios": "^1.6.0",
    "chart.js": "^4.4.0",
    "dayjs": "^1.11.10",
    "lodash": "^4.17.21",
    "sweetalert2": "^11.7.32",
    "underscore": "^1.13.6"
  }
}
